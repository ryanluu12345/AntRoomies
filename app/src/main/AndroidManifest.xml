<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ryanluu2017.roomright">

    <application
        android:largeHeap="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignInActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".VerifyEmailActivity" />
        <activity android:name=".UserPreferencesActivity" />
        <activity android:name=".UserNumericalPreferencesActivity" />
        <activity android:name=".UserProfileActivity" />
        <activity android:name=".BrowseUsersActivity" />
        <activity android:name=".SwipeBrowseUsersActivity"></activity>
    </application>

</manifest>